{
    "manifest_version": 3,
    "name": "Download Web Code",
    "description": "Downloads HTML, CSS, and JS from the website.",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "downloads",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  